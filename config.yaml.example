# Example configuration file
vllm:
  api_url: "http://localhost:8002/v1/chat/completions"
  model: "Qwen/Qwen2.5-Coder-32B-Instruct-AWQ"
  timeout: 180
  max_retries: 3

security:
  max_file_size: 1048576  # 1MB
  max_response_length: 50000
  allowed_paths:
    - "."

logging:
  enabled: true
  level: "INFO"
  file: "./logs/delegator.log"

features:
  caching: true
  metrics: true
  auto_backup: true
